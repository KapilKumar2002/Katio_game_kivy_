<GameFinishMessage>:
    orientation: "vertical"
    spacing: dp(15)
    size_hint: 1, None
    height: dp(180)
    padding: dp(25)
    MDLabel:
        text: "Katio"
        halign: "center"
        font_weight: "bold"
        font_size: dp(30)
        font_name: "Bpoppins"
        color: 41/255, 227/255, 196/255
    MDIcon:
        icon: "account"
        theme_text_color: "Custom"
        text_color: root.color 
        font_size: dp(40)
        pos_hint: {"center_x": .5}
    MDBoxLayout:
        pos_hint: {"center_x": .5}
        spacing: dp(8)
        MDIcon:
            icon: "circle"
            theme_text_color: "Custom"
            text_color: root.color 
